{
  "name": "letovo-admin-frontend",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Letovo School - Administration panel",
  "repository": {
    "type": "git",
    "url": "https://github.com/junto-team/Letovo-Admin-Frontend.git"
  },
  "scripts": {
    "dev": "yarn makesvg && razzle start",
    "build": "yarn makesvg && razzle build",
    "start": "cross-env NODE_ENV=production node build/server.js",
    "test": "razzle test --env=jsdom",
    "makesvg": "gulp svg-sprite",
    "lint": "eslint -c ./.eslintrc.js --ext .js,.jsx,.ts,.vue ./src"
  },
  "jest": {
    "testMatch": [
      "**/?(*.)(spec|test).(js|ts)?(x)"
    ],
    "moduleNameMapper": {
      "^@/(.*)": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "^.+\\.[t|j]sx?$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "ckeditor4-vue": "^1.3.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dayjs": "^1.8.33",
    "effector": "^21.2.2",
    "effector-root": "^0.2.0",
    "effector-vue": "^21.0.3",
    "express": "^4.17.1",
    "js-cookie": "^2.2.1",
    "patronum": "^0.106.0",
    "razzle": "^3.1.3",
    "serialize-javascript": "^5.0.1",
    "simplebar": "^5.3.0",
    "swiper": "^6.5.1",
    "v-tooltip": "^2.1.2",
    "vue": "^2.6.11",
    "vue-awesome-swiper": "^4.1.1",
    "vue-click-outside": "^1.1.0",
    "vue-color": "^2.8.1",
    "vue-events": "^3.1.0",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.3.4",
    "vue2-datepicker": "^3.9.0",
    "vuedraggable": "^2.24.3",
    "vuetable-2": "^2.0.0-beta.4"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/eslint-parser": "^7.12.13",
    "@babel/preset-typescript": "^7.10.4",
    "@types/express": "^4.17.6",
    "@types/express-http-proxy": "^1.6.1",
    "@types/jest": "^26.0.3",
    "@types/js-cookie": "^2.2.6",
    "@types/node": "^14.0.14",
    "@types/serialize-javascript": "^5.0.0",
    "@types/vue-color": "^2.4.3",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "autoprefixer": "^9.8.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-vue": "^2.0.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier-vue": "^2.1.1",
    "eslint-plugin-vue": "^6.2.2",
